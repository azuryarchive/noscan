{
  "name": "noscan",
  "version": "1.0.0",
  "description": "A more flexible SDK for unscan",
  "main": "build/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --minify --outfile=build/index.js",
    "watch": "esbuild src/index.ts --bundle --platform=node --watch --minify --outfile=build/index.js",
    "test": "jest",
    "publish:patch": "npm run minify && npm version patch && npm publish",
    "publish:minor": "npm run minify && npm version minor && npm publish",
    "publish:major": "npm run minify && npm version major && npm publish"
  },
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/azury"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azurystudios/noscan.git"
  },
  "keywords": [
    "sdk",
    "scan",
    "malware-detection",
    "minimalistic",
    "easy-to-use",
    "asynchronous"
  ],
  "author": "Azury <hello@azury.dev> (https://azury.dev)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/azurystudios/noscan/issues"
  },
  "homepage": "https://github.com/azurystudios/noscan#readme",
  "devDependencies": {
    "@types/bent": "^7.3.2",
    "@types/node": "^16.11.7",
    "boxen": "^6.2.1",
    "chalk": "^5.0.0",
    "esbuild": "^0.14.0",
    "jest": "^27.4.0",
    "node-fetch": "^3.1.0",
    "terser": "^5.9.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
